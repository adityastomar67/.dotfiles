# Safety function for printing icons
# Prints the named icon, or if that icon is undefined, the string name.
function print_icon() {
  local icon_name=$1
  local ICON_USER_VARIABLE=${icon_name}
  echo -n "${icons[$icon_name]}"
}

get_icon_names() {
  for key in ${(@k)icons}; do
    echo "$key: ${icons[$key]}"
  done
}
